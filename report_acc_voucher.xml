<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <report
            id="report_account_voucher"
            model="account.voucher"
            string="Account Voucher Report"
            report_type="qweb-pdf"
            file="pj_test.report_voucher_template"
            name="pj_test.report_voucher_template"

        />
        <record id="paperformat_voucher" model="report.paperformat">
            <field name="name">Account Voucher</field>
            <field name="default" eval="True" />
            <field name="format">A4</field>
            <field name="page_height">0</field>
            <field name="page_width">0</field>
            <field name="orientation">Portrait</field>
            <field name="margin_top">100</field>
            <field name="margin_bottom">10</field>
            <field name="margin_left">7</field>
            <field name="margin_right">7</field>
            <field name="header_line" eval="False" />
            <field name="header_spacing">95</field>
            <field name="dpi">90</field>

        </record>
        <record id="report_account_voucher_template" model="ir.actions.report.xml">
            <field name="paperformat_id" ref="pj_test.paperformat_voucher"/>
        </record>
    </data>
</openerp>